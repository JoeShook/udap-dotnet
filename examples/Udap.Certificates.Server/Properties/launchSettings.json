{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "http://host.docker.internal:5033",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_Kestrel__Endpoints__Https__Url": "http://host.docker.internal:5033"
      },
      "notes": [
        "This converation allowed me to bind to host.docker.internal in Aspire when not running in a container on Windows.",
        "https://github.com/dotnet/aspire/issues/1116#issuecomment-1879818156"          
      ]
    }
  }
}
