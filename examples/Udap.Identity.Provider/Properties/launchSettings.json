{
  "profiles": {
    "Localhost": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "GCPDeploy": "false",
        "ASPNETCORE_ENVIRONMENT": "Development",
        "UdapIdpBaseUrl": "https://localhost:5055"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:5055",
      "sslPort": 5055
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:443;http://+:80"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}