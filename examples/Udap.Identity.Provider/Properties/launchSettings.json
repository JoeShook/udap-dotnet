{
  "profiles": {
    "Localhost": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "https://host.docker.internal:5055",
      "environmentVariables": {
        "GCPDeploy": "false",
        "ASPNETCORE_ENVIRONMENT": "Development",
        "UdapIdpBaseUrl": "https://host.docker.internal:5055",
        "ASPNETCORE_Kestrel__Endpoints__Https__Url": "https://host.docker.internal:5055"
      },
      "notes": [
        "This converation allowed me to bind to host.docker.internal in Aspire when not running in a container on Windows.",
        "https://github.com/dotnet/aspire/issues/1116#issuecomment-1879818156"
      ]
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://host.docker.internal:{ServicePort}",
      "useSSL": true,
      "environmentVariables": {
        "GCPDeploy": "false",
        "ASPNETCORE_URLS": "http://*:80;https://*:443",
        "ASPNETCORE_ENVIRONMENT": "Development",
        "UdapIdpBaseUrl": "https://host.docker.internal:5055"
      }
    }
  }
}
